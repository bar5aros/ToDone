{
    "env": {
        "browser": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension" : "off",
        "linebreak-style" : ["error", "windows"],
        "no-use-before-define": ["error", { "variables": false }],
        "camelcase" :"off"
    }
}
